{
  "name": "it-hosting",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "vercel-build": "npm ci --include=dev && prisma generate && next build",
    "prisma:generate": "prisma generate --schema=./prisma/schema.prisma",
    "postinstall": "npm run prisma:generate",
    "start": "next start",
    "lint": "next lint",
    "test": "playwright test --headed --timeout 120000",
    "test:ci": "playwright test  --retries=3 --timeout 120000 --workers 2"
  },
  "dependencies": {
