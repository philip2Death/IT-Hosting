    "@tailwindcss/postcss": "^4.0.17",
    "@types/eslint": "^8.56.5",
    "@types/react": "19.1.1",
    "@types/react-dom": "^19.1.0",
    "aws-sdk": "^2.1692.0",
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "eslint": "^9.0.0",
    "mock-aws-s3": "^4.0.2",
    "next": "15.2.4",
    "next-auth": "^4.24.11",
    "nock": "^14.0.3",
    "postcss": "^8.5.3",
    "ratelimiter": "^3.4.1",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
